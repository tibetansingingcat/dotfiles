{
  "React Arrow Function Component (named)": {
    "prefix": "rafc",
    "body": [
      "const ${1:ComponentName} = () => {",
      "  return (",
      "    <>",
      "      $0",
      "    </>",
      "  )",
      "}",
      "",
      "export default ${1:ComponentName}"
    ],
    "description": "React Arrow Function Component with prompted name"
  },
  "React Arrow Function Component with Props (named)": {
    "prefix": "rafcp",
    "body": [
      "interface ${1:ComponentName}Props {",
      "  $2",
      "}",
      "",
      "const ${1:ComponentName} = ({ $3 }: ${1:ComponentName}Props) => {",
      "  return (",
      "    <>",
      "      $0",
      "    </>",
      "  )",
      "}",
      "",
      "export default ${1:ComponentName}"
    ],
    "description": "React Arrow Function Component with Props interface"
  },
